{
  "name": "@aspnyan/discord-handlers",
  "version": "1.1.0",
  "description": "A JavaScript/TypeScript module assisting with creating REST & Non-REST Event Handlers, as well as assisting with creating Slash Commands for your Discord bot",
  "module": "./build/ESM/exports.mjs",
  "types": "./build/types.d.ts",
  "main": "./build/CJS/exports.cjs",
  "exports": {
    ".": {
      "require": "./build/exports.cjs",
      "import": "./build/exports.mjs",
      "types": "./build/types.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ASPNyan/discord-handlers.git"
  },
  "author": "ASPNyan",
  "license": "CC-BY-NC-SA-4.0",
  "bugs": {
    "url": "https://github.com/ASPNyan/discord-handlers/issues"
  },
  "homepage": "https://github.com/ASPNyan/discord-handlers#readme",
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "discord.js": "^14.3.0",
    "glob": "^8.0.3"
  },
  "dependencies": {
    "ascii-table3": "^0.7.7",
    "copyfiles": "^2.4.1",
    "typescript": "^4.8.3",
    "renamer": "^4.0.0"
  },
  "files": [
    "build",
    "LICENSE.md",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build-types": "powershell Copy-Item -Path \".\\dev\\types.d.ts\" -Destination \".\\build\"",
    "build-files": "npx tsc -p ./tsconfig.json && npx tsc -p ./tsconfig-cjs.json",
    "rename-esm": "npx renamer -regex --find '\\.js^' --replace '.mjs' './build/ESM/**/*.js",
    "rename-cjs": "npx renamer -regex --find '\\.js^' --replace '.cjs' './build/CJS/**/*.js",
    "build": "npm run build-files && npm run build-types && npm run rename-esm && npm run rename-cjs"
  }
}
